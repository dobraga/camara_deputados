{
    "partidos": {
        "from": "partidos"
    },

    "partidos_membros": {
        "id": "partidos",
        "from": "partidos.membros",
        "where": {
            "id": "{id}",
            "dataInicio": "{dataInicio}",
            "dataFim": "{dataFim}"
        }
    },
    
    "deputados": {
        "id": "partidos_membros",
        "from": "deputados",
        "where": {
            "id": "{id}"
        }
    }
}